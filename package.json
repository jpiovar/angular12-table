{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --port 8080",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildJar": "ng build --configuration=production --output-hashing=all --base-href /ui/ --output-path ../../target/dist",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock": "json-server --port 3000 --watch ./src/assets/mock/server.js --routes ./src/assets/mock/routes.json",
    "db": "json-server --no-cors --port 3000 --watch ./src/assets/mock/db.json",
    "postinstall": "ngcc",
    "preinstall": "npm install -g node-gyp",
    "stylelint": "stylelint **/*.scss",
    "project:init": "npm cache clean --force && npm cache verify && npm i -g @angular/cli && npm i -g @angular/compiler && npm i -g @angular/compiler-cli && npm i -g typescript && npm install",
    "lint:staged": "run-with-changes i -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "lint:changed": "run-with-changes iwu -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "tslint-files": "cd ./../../ && tslint",
    "stylelint-files": "cd ./../../ && stylelint",
    "start": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/material-moment-adapter": "^12.2.13",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@azure/msal-angular": "^2.1.2",
    "@azure/msal-browser": "^2.22.1",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@plantandgo/helpers": "^1.0.7",
    "bootstrap": "^4.6.1",
    "classlist.js": "^1.1.20150312",
    "dompurify": "^2.3.6",
    "express": "^4.17.3",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.5.0",
    "moment": "^2.29.1",
    "natural-orderby": "^2.0.3",
    "ngx-device-detector": "^2.2.1",
    "ngx-toastr": "^14.2.2",
    "postcss": "^8.4.8",
    "rxjs": "~6.6.0",
    "serve-static": "^1.14.2",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.21.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.3",
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/localize": "^12.2.16",
    "@types/jasmine": "~3.8.2",
    "@types/lodash": "^4.14.179",
    "@types/node": "^12.20.47",
    "codelyzer": "^6.0.2",
    "dotenv-webpack": "^7.1.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "protractor": "~7.0.0",
    "run-with-changes": "^1.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
