{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --port 8080",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildJar": "ng build --configuration=production --output-hashing=all --base-href /ui/ --output-path ../../target/dist",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock": "json-server --port 3000 --watch ./src/assets/mock/server.js --routes ./src/assets/mock/routes.json",
    "db": "json-server --no-cors --port 3000 --watch ./src/assets/mock/db.json",
    "postinstall": "ngcc",
    "preinstall": "npm install -g node-gyp",
    "stylelint": "stylelint **/*.scss",
    "project:init": "npm cache clean --force && npm cache verify && npm i -g @angular/cli && npm i -g @angular/compiler && npm i -g @angular/compiler-cli && npm i -g typescript && npm install",
    "lint:staged": "run-with-changes i -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "lint:changed": "run-with-changes iwu -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "tslint-files": "cd ./../../ && tslint",
    "stylelint-files": "cd ./../../ && stylelint",
    "start": "node server.js"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@azure/msal-angular": "^2.4.1",
    "@azure/msal-browser": "^2.28.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.1",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@plantandgo/helpers": "^1.0.7",
    "autoprefixer": "10.4.5",
    "bootstrap": "^4.6.2",
    "dompurify": "^2.3.10",
    "express": "^4.18.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.7.0",
    "moment": "^2.29.4",
    "natural-orderby": "^2.0.3",
    "ngx-device-detector": "^2.2.1",
    "ngx-toastr": "^14.3.0",
    "postcss": "^8.4.16",
    "rxjs": "~6.6.0",
    "serve-static": "^1.15.0",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.21.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/localize": "^13.3.11",
    "@types/jasmine": "~3.8.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.55",
    "codelyzer": "^6.0.0",
    "dotenv-webpack": "^7.1.1",
    "husky": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "protractor": "~7.0.0",
    "run-with-changes": "^1.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
